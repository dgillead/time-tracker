<h1 class="text-center">Your Projects</h1>
<div class="container">
  <ul class="list-group">
  <% projects.each do |project|  %>
    <a href="/projects/<%= project.id %>/view"><li class="text-center center-block list-group-item"><%= project.name %></li></a>
  <% end %>
  </ul>

  <div class="text-center">
    <a href="/projects"><button class="btn btn-success">Create New Project</button>
    <a href="/work_sessions/<%= user.id %>/list_user"><button class="btn btn-info">See Your Work Sessions</button></a>
    <a href="/users/<%= current_user.id %>"><button class="btn btn-primary">Your Account</button></a>
  </div>
</div>
